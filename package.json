{
  "name": "ember-cli-graphql-file",
  "version": "0.0.1",
  "description": "An addon to precompile your `.graphql` files with `graphql-tag` and turn them into importable modules.",
  "keywords": [
    "ember-addon",
    "graphql",
    "graphql-tag"
  ],
  "license": "BSD-3-Clause",
  "author": "Charles Demers <cdemers@mirego.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/mirego/ember-cli-graphql-file",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test"
  },
  "dependencies": {
    "ember-cli-babel": "5.2.2",
    "graphql-tag": "1.2.4"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "broccoli-asset-rev": "2.5.0",
    "broccoli-persistent-filter": "1.2.13",
    "ember-cli": "2.11.0",
    "ember-cli-dependency-checker": "1.3.0",
    "ember-cli-htmlbars": "1.1.1",
    "ember-cli-htmlbars-inline-precompile": "0.3.6",
    "ember-cli-qunit": "3.1.1",
    "ember-cli-shims": "1.0.2",
    "ember-cli-test-loader": "1.1.1",
    "ember-cli-uglify": "1.2.0",
    "ember-disable-prototype-extensions": "1.1.0",
    "ember-load-initializers": "0.6.3",
    "ember-resolver": "2.1.1",
    "ember-source": "2.11.0",
    "eslint": "3.15.0",
    "loader.js": "4.1.0"
  },
  "engines": {
    "node": ">= 6.9.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-babel"
  }
}
